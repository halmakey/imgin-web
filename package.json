{
  "name": "imgin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "bunx @cloudflare/next-on-pages",
    "preview": "bun pages:build && wrangler pages dev",
    "deploy": "bun pages:build && wrangler pages deploy --branch=production",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "migrate:local": "wrangler d1 migrations apply imgin-db --local",
    "migrate:remote": "wrangler d1 migrations apply imgin-db --remote"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "date-fns": "^3.6.0",
    "nanoid": "^5.0.7",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "webm-muxer": "^5.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "^4.20240815.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-next-on-pages": "^1.13.2",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.4",
    "vercel": "^36.0.0",
    "wrangler": "^3.72.0"
  }
}
